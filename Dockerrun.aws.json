{
  "AWSEBDockerrunVersion": 3,
  "containerDefinitions": [
    {
      "name": "frontend",
      "image": "954587698208.dkr.ecr.eu-west-3.amazonaws.com/soccer-data-frontend",
      "hostname": "frontend",
      "essential": true,
      "memory": 512,
      "portMappings": [
        {
          "hostPort": 5173,
          "containerPort": 5173
        }
      ]
    },
    {
      "name": "backend",
      "image": "954587698208.dkr.ecr.eu-west-3.amazonaws.com/soccer-data-backend",
      "hostname": "backend",
      "essential": true,
      "memory": 1024,
      "portMappings": [
        {
          "hostPort": 3001,
          "containerPort": 3001
        }
      ]
    },
    {
      "name": "nginx",
      "image": "954587698208.dkr.ecr.eu-west-3.amazonaws.com/soccer-data-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [
        {
          "hostPort": 80,
          "containerPort": 80
        }
      ],
      "links": [
        "backend",
        "frontend"
      ],
      "memory": 512
    }
  ]
}